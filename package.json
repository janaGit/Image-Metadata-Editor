{
  "name": "image-metadata-editor",
  "version": "2.0.0",
  "description": "Editor to modify metadata of many images in a fast way.",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --proxy-config proxy.conf.json",
    "start-no-debug": "ng serve --open --proxy-config proxy.conf.json --source-map=false --verbose --no-progress",
    "build": "ng build && npm run server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server": "node server/dist/index",
    "dev-server": "cd server && npm run webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janaGit/Image-Metadata-Editor.git"
  },
  "keywords": [
    "metadata",
    "images",
    "pictures",
    "editor"
  ],
  "author": "Jana Klemp",
  "private": true,
  "bugs": {
    "url": "https://github.com/janaGit/Image-Metadata-Editor/issues"
  },
  "homepage": "https://github.com/janaGit/Image-Metadata-Editor#readme",
  "dependencies": {
    "@angular/animations": "10.0.10",
    "@angular/cdk": "10.1.3",
    "@angular/common": "10.0.10",
    "@angular/compiler": "10.0.10",
    "@angular/core": "11.0.5",
    "@angular/forms": "10.0.10",
    "@angular/material": "10.1.3",
    "@angular/platform-browser": "10.0.10",
    "@angular/platform-browser-dynamic": "10.0.10",
    "@angular/platform-server": "10.0.10",
    "@angular/router": "10.0.10",
    "@fortawesome/fontawesome": "1.1.8",
    "@fortawesome/fontawesome-free-solid": "5.0.13",
    "bootstrap": "3.4.1",
    "leaflet": "1.6.0",
    "material-design-icons": "3.0.1",
    "multer": "1.4.2",
    "ngx-bootstrap": "5.6.1",
    "rxjs": "6.6.2",
    "rxjs-compat": "6.6.2",
    "tslib": "2.0.1",
    "typeface-roboto": "0.0.75",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.6",
    "@angular/cli": "10.0.6",
    "@angular/compiler-cli": "10.0.10",
    "@angular/language-service": "10.0.10",
    "@types/body-parser": "1.19.0",
    "@types/express": "4.17.7",
    "@types/jasmine": "3.5.12",
    "@types/multer": "1.4.4",
    "@types/node": "14.6.0",
    "codelyzer": "6.0.0",
